{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "[dev|watch] Launch Chrome against localhost",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceFolder}/dist/index.html",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceFolder}/src/*",
      },
    },
  ]
}
